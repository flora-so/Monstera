{
  "name": "monsetra-vue",
  "version": "0.5.5",
  "description": "Monsetra is a Vue 3 UI Library based on the Monsetra Design Principles",
  "license": "MIT",
  "keywords": [
    "monsetra",
    "monsetra-vue",
    "vue",
    "vue3",
    "typescript",
    "ui",
    "library",
    "framework",
    "design"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/vuelib.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/button/*",
    "dist/content/*",
    "dist/overlay/*",
    "dist/input/*",
    "dist/compose/*",
    "dist/cjs/*",
    "dist/esm/*",
    "dist/types/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pilea-eco/Monsetra"
  },
  "scripts": {
    "dev": "vite",
    "build:setup": "rm -rf dist && npm run typecheck",
    "build:types": "tsc --build tsconfigtypes.json",
    "build": "npm run build:setup && rollup -c rollup.config.js -f es && npm run build:types",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@rollup/plugin-url": "^6.1.0",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "minimist": "^1.2.6",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "postcss-url": "^10.1.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  }
}