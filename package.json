{
  "name": "monsetra-vue-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm --prefix ./package/ run build",
    "deploy:patch": "npm --prefix ./package/ run deploy:patch",
